{
  "name": "shopit-be",
  "version": "1.0.0",
  "description": "Shopit!",
  "main": "app.js",
  "scripts": {
    "start": "npm run build && NODE_ENV=DEVELOPMENT && node dist/server.js",
    "dev": "npm run build && NODE_ENV=DEVELOPMENT nodemon --watch '**/*.ts' --exec 'ts-node' ./src/server.ts",
    "clean": "rm -rf ./dist",
    "build": "tsc",
    "seeder:sql": "npm run clean && npm run build && NODE_ENV=DEVELOPMENT && node dist/shared/db/sql/seeder.js",
    "seeder:noSql": "npm run clean && npm run build && NODE_ENV=DEVELOPMENT && node dist/shared/db/noSql/seeder.js",
    "test": "mocha --timeout 10000 --require ts-node/register test/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fitu/shopit-be.git"
  },
