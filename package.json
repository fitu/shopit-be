{
  "name": "shopit-be",
  "version": "1.0.0",
  "description": "Shopit!",
  "main": "app.js",
  "scripts": {
    "start": "npm run build && NODE_ENV=DEVELOPMENT && node dist/server.js",
    "dev": "npm run build && NODE_ENV=DEVELOPMENT nodemon --watch '**/*.ts' --exec 'ts-node' ./src/server.ts",
    "clean": "rm -rf ./dist",
    "build": "tsc",
    "seeder:sql": "npm run clean && npm run build && NODE_ENV=DEVELOPMENT && node dist/shared/db/sql/seeder.js",
    "seeder:noSql": "npm run clean && npm run build && NODE_ENV=DEVELOPMENT && node dist/shared/db/noSql/seeder.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fitu/shopit-be.git"
  },
  "author": "Victorio Matteucci",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fitu/shopit-be/issues"
  },
  "homepage": "https://github.com/fitu/shopit-be#readme",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.11",
    "nodemon": "^2.0.14",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "connect-mongodb-session": "^3.1.1",
    "connect-session-sequelize": "^7.1.2",
    "csv-parser": "^3.0.0",
    "dotenv": "^10.0.0",
    "envalid": "^7.2.2",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "http-status": "^1.5.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "mongodb": "^4.2.2",
    "mongoose": "^6.1.5",
    "pg": "^8.7.1",
    "sequelize": "^6.12.0-alpha.1"
  }
}
