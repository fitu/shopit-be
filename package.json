  "bugs": {
    "url": "https://github.com/fitu/shopit-be/issues"
  },
  "homepage": "https://github.com/fitu/shopit-be#readme",
  "devDependencies": {
    "@faker-js/faker": "^6.3.1",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.178",
    "@types/mocha": "^9.1.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.11",
    "nodemon": "^2.0.14",
    "supertest": "^6.2.2",
    "ts-node": "^10.5.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@types/chai": "^4.3.0",
